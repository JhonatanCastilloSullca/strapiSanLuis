{
  "kind": "collectionType",
  "collectionName": "cabeceras",
  "info": {
    "singularName": "cabecera",
    "pluralName": "cabeceras",
    "displayName": "Cabecera"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "imagen": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "titulo": {
      "type": "string",
      "required": true
    },
    "subtitulo": {
      "type": "string"
    },
    "posicion": {
      "type": "enumeration",
      "enum": [
        "izquierda",
        "centro",
        "derecha"
      ],
      "default": "centro"
    },
    "linkAction": {
      "type": "string"
    }
  }
}
